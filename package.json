{
  "name": "svelte-navigator",
  "version": "1.1.0",
  "description": "Simple, declarative routing for single page apps built with Svelte",
  "main": "dist/svelte-navigator.umd.js",
  "unpkg": "dist/svelte-navigator.umd.min.js",
  "module": "dist/svelte-navigator.module.mjs",
  "svelte": "src/index.js",
  "files": [
    "src",
    "dist"
  ],
  "author": "Michel Strelow @mefechoel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mefechoel/svelte-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/mefechoel/svelte-navigator/issues"
  },
  "homepage": "https://github.com/mefechoel/svelte-navigator#readme",
  "keywords": [
    "svelte-navigator",
    "svelte",
    "router",
    "routing",
    "navigate",
    "navigator",
    "spa",
    "declarative routing",
    "hooks"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest test --coverage",
    "test:watch": "jest test --watch",
    "cy:open": "cypress open",
    "cy:build": "rollup -c test/testApp/rollup.config.js",
    "cy:serve": "sirv test/testApp/public --single --port 7070",
    "cy:start": "yarn cy:build && yarn cy:serve",
    "cy:run": "cypress run",
    "cy": "yarn start-server-and-test cy:start http://localhost:7070 cy:run"
  },
  "peerDependencies": {
    "svelte": "3.x"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@cypress/code-coverage": "^3.7.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^4.5.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.5.4",
    "nyc": "^15.0.1",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^2.7.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^0.4.5",
    "start-server-and-test": "^1.11.0",
    "svelte": "^3.20.1",
    "svelte-jester": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.m?jsx?$",
    "moduleFileExtensions": [
      "js",
      "mjs",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
